
public class vetor {
	private double x;
	private double y;
	
	public vetor()
	{
		x = 0;
		y = 0;
	}

	public vetor(double xx, double yy)
	{
		this.x = xx;
		this.y = yy;
	}
	
	public vetor(double x)
	{
		this.x = x;
		this.y = 0;
	}
	
	{
		System.out.println("x: " + this.x + "\n" + "y: " + this.y);
	}
}
